      "source": [
        "class Solution:\n",
        "    def angleClock(self, hour: int, minutes: int) -> float:\n",
        "        if hour==12 and minutes==0:\n",
        "            return 0\n",
        "        a=(30*hour)+(minutes/2)\n",
        "        b=(minutes*6)\n",
        "        c=abs(a-b)\n",
        "        if c<180:\n",
        "            return c\n",
        "        else:\n",
        "            return 360-c"
      ]
